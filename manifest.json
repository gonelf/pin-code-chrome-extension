{
  "manifest_version": 2,
 
  "name": "Code Pin",
  "description": "Pin your favorite code",
  "version": "1.0",
 
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "CodePin.html"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "permissions": ["activeTab", "tabs"]
}